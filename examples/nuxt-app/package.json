{
  "private": true,
  "packageManager": "pnpm@7.23.0",
  "license": "Apache-2.0",
  "name": "nuxt-app",
  "version": "2.1.0-alpha.75",
  "scripts": {
    "dev": "nuxi dev",
    "dev:debug": "node --inspect node_modules/.bin/nuxi dev",
    "dev:mock": "NUXT_PUBLIC_API_URL=http://localhost:3001 concurrently 'pnpm mockserver' 'pnpm dev'",
    "build": "nuxi build",
    "compile": "tsc --build",
    "start": "node .output/server/index.mjs",
    "mockserver": "node ./mockserver.js",
    "start:mock": "NUXT_PUBLIC_API_URL=http://localhost:3001 concurrently 'pnpm mockserver' 'pnpm start'",
    "cy:open": "cypress open --e2e --browser electron",
    "cy:run": "cypress run",
    "test:ci": "NUXT_PUBLIC_API_URL=http://localhost:3001 start-test start 'http-get://localhost:3000/api/tide/site?id=8888' 'cy:run'",
    "test:integration": "start-test 'NUXT_PUBLIC_API_URL=http://localhost:3001 pnpm dev' 'http-get://localhost:3000/api/tide/site?id=8888' 'cy:open'",
    "lighthouse": "unlighthouse-ci --site http://localhost:3000",
    "lighthouse:ci": "start-test start:mock 3000 lighthouse"
  },
  "dependencies": {
    "@dpc-sdp/nuxt-ripple": "2.1.0-alpha.75",
    "@dpc-sdp/ripple-tide-api": "2.1.0-alpha.74",
    "@dpc-sdp/ripple-tide-event": "2.1.0-alpha.74",
    "@dpc-sdp/ripple-tide-grant": "2.1.0-alpha.74",
    "@dpc-sdp/ripple-tide-landing-page": "2.1.0-alpha.75",
    "@dpc-sdp/ripple-tide-media": "2.1.0-alpha.75",
    "@dpc-sdp/ripple-tide-news": "2.1.0-alpha.75",
    "@dpc-sdp/ripple-tide-publication": "2.1.0-alpha.75",
    "@dpc-sdp/ripple-tide-search": "2.1.0-alpha.75",
    "@dpc-sdp/ripple-ui-core": "2.1.0-alpha.71",
    "@dpc-sdp/ripple-ui-forms": "2.1.0-alpha.75",
    "@formkit/core": "1.0.0-beta.12",
    "@formkit/vue": "1.0.0-beta.12"
  },
  "devDependencies": {
    "@badeball/cypress-cucumber-preprocessor": "^15.1.0",
    "@nuxt/vite-builder": "^3.0.0",
    "@testing-library/cypress": "^8.0.7",
    "@unlighthouse/cli": "^0.4.9",
    "mockttp": "^3.6.1",
    "nuxt": "3.0.0",
    "ohmyfetch": "^0.4.21",
    "start-server-and-test": "^1.15.2",
    "ufo": "^1.0.1",
    "vue": "^3.2.45",
    "vue-router": "^4.1.6"
  }
}
