    "@dpc-sdp/ripple-tide-landing-page": "2.1.0-alpha.55",
    "@dpc-sdp/ripple-tide-media": "2.1.0-alpha.55",
    "@dpc-sdp/ripple-tide-news": "2.1.0-alpha.55",
    "@dpc-sdp/ripple-tide-publication": "2.1.0-alpha.55"
  },
  "devDependencies": {
    "@badeball/cypress-cucumber-preprocessor": "^14.0.0",
    "@nuxt/vite-builder": "^3.0.0",
    "@testing-library/cypress": "^8.0.7",
    "mockttp": "^3.6.1",
    "nuxt": "3.0.0",
    "start-server-and-test": "^1.15.0"
