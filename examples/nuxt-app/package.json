  "name": "nuxt-app",
  "version": "2.1.0-alpha.55",
  "scripts": {
    "dev": "nuxi dev",
    "dev:debug": "node --inspect node_modules/.bin/nuxi dev",
    "build": "nuxi build",
    "compile": "tsc --build",
    "start": "node .output/server/index.mjs",
    "mockserver": "node ./mockserver.js",
    "start:mock": "API_URL=http://localhost:3001 concurrently 'yarn mockserver' 'yarn start'",
    "cy:open": "cypress open --e2e --browser electron",
    "cy:run": "cypress run",
    "test:integration": "start-test 'NUXT_PUBLIC_API_URL=http://localhost:3001 yarn dev' http-get://localhost:3000/api/tide/site?id=8888 'cy:open'"
  },
  "dependencies": {
    "@dpc-sdp/nuxt-ripple": "2.1.0-alpha.55",
    "@dpc-sdp/ripple-tide-event": "2.1.0-alpha.55",
