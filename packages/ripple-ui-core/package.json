{
  "name": "@dpc-sdp/ripple-ui-core",
  "version": "0.0.0",
  "files": [
    "dist"
  ],
  "main": "./dist/ripple-ui-core.umd.js",
  "module": "./dist/ripple-ui-core.es.js",
  "typings": "./dist-types/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/ripple-ui-core.es.js",
      "require": "./dist/ripple-ui-core.umd.js"
    }
  },
  "scripts": {
    "dev": "vite",
    "build": "yarn clean && yarn types && vite build",
    "types": "vue-tsc --declaration --emitDeclarationOnly",
    "watch:build": "yarn types && vite build --watch",
    "clean": "rimraf dist* && rimraf tsconfig.tsbuildinfo",
    "serve": "vite preview"
  },
  "dependencies": {
    "vue": "^3.2.16"
  },
  "publishConfig": {
    "main": "./dist/ripple-ui-core.umd.js"
  },
  "devDependencies": {
    "@vitejs/plugin-vue": "^1.9.3",
    "postcss-nested": "^5.0.6",
    "rollup-plugin-copy": "^3.4.0",
    "rollup-plugin-delete": "^2.0.0",
    "vite": "^2.6.4",
    "vue-tsc": "^0.31.2"
  }
}
