{
  "packageManager": "pnpm@8.6.2",
  "name": "ripple-storybook",
  "description": "Ripple Storybook instance",
  "version": "2.1.0-alpha.193",
  "license": "Apache-2.0",
  "private": true,
  "scripts": {
    "start": "storybook dev -p 6006",
    "static:build": "storybook build",
    "static:start": "http-server storybook-static --port 6006 --silent",
    "static:test": "wait-on tcp:6006 && test-storybook --stories-json --maxWorkers=2",
    "test:ci": "concurrently -k -s first -n \"SB,TEST\" -c \"magenta,blue\" \"pnpm static:start\" \"pnpm static:test\""
  },
  "devDependencies": {
    "@storybook/addon-a11y": "^7.0.22",
    "@storybook/addon-actions": "^7.0.22",
    "@storybook/addon-essentials": "^7.0.22",
    "@storybook/addon-interactions": "^7.0.22",
    "@storybook/addon-jest": "^7.0.22",
    "@storybook/addon-links": "^7.0.22",
    "@storybook/blocks": "^7.0.22",
    "@storybook/jest": "^0.1.1-future.2",
    "@storybook/mdx1-csf": "^1.0.0",
    "@storybook/test-runner": "^0.11.0-future.5",
    "@storybook/testing-library": "^0.1.1-future.2",
    "@storybook/vue3": "^7.0.22",
    "@storybook/vue3-vite": "^7.0.22",
    "concurrently": "^8.2.0",
    "http-server": "^14.1.1",
    "jest-axe": "^7.0.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "storybook": "^7.0.22",
    "wait-on": "^7.0.1"
  },
  "dependencies": {
    "vite-svg-loader": "^4.0.0"
  }
}
